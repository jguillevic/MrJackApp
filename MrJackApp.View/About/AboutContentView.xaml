<UserControl x:Class="MrJackApp.View.About.AboutContentView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Width="400"
             Height="400">
    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/MrJackApp.View;component/Common/ResDic/CommonResDic.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
    <Grid>
        <Rectangle Fill="Black" Opacity="0.6"/>
        <Grid Margin="10 10 0 0">
            <TextBlock Text="A propos de Mr. Jack" Style="{StaticResource BigTextStyle}"/>
        </Grid>
    </Grid>
</UserControl>
